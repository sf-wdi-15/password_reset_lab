Please reset your email the link below

<%= link_to "Reset Password", edit_password_url(@user.code) %>

<%= @user.email %>
<%= form_for :user, url: "/passwords/#{@user.code}" do |f| %>
  <%= f.password_field :password %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit "Update" %>
<% end %>